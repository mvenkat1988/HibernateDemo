<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.bean.EmployeeBean" table="Emp_tb">
		<id name="eid" column="e_id">
			<generator class="assigned">
			</generator>
		</id>
		<property name="lastName" column="e_last_Name" />
		<property name="firstName" column="e_first_Name" />
		<property name="address" column="e_address" />
		<property name="age" column="e_age" />
		<property name="phoneNum" column="e_phoneNum" />
	</class>
	
	
</hibernate-mapping>